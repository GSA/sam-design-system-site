{
  "name": "sam-cli",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run init && ng serve --preserve-symlinks",
    "start:aot": "npm run init && ng serve --preserve-symlinks --aot",
    "build": "npm run init && ng build --preserve-symlinks --aot",
    "test": "npm run init && ng test",
    "lint": "ng lint",
    "e2e": "npm run init && ng e2e",
    "server:prod": "nginx -g 'daemon off;'",
    "prod": "npm run set-env && npm run server:prod",
    "generate-docs": "node ./config/docgen.js",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' --json './src/assets/docs.json' ./node_modules/sam-ui-elements/src/ui-kit",
    "typedoc": "./node_modules/typedoc/bin/typedoc",
    "compile-markdown": "node src/environments/compile-markdown.js",
    "set-env": "node src/environments/set-env.js",
    "init": "npm run compile-markdown && npm run set-env && npm run docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.3",
    "@angular/common": "4.3.3",
    "@angular/compiler": "4.3.3",
    "@angular/cli": "1.4.10",
    "@angular/compiler-cli": "4.3.3",
    "@angular/core": "4.3.3",
    "@angular/forms": "4.3.3",
    "@angular/http": "4.3.3",
    "@angular/platform-browser": "4.3.3",
    "@angular/platform-browser-dynamic": "4.3.3",
    "@angular/router": "4.3.3",
    "classlist-polyfill": "1.0.3",
    "font-awesome": "4.7.0",
    "core-js": "^2.4.1",
    "markdown-it": "8.4.0",
    "moment": "2.20.1",
    "prismjs": "1.12.2",
    "recursive-readdir-sync": "1.0.6",
    "rxjs": "^5.4.2",
    "typedoc": "0.5.10",
    "typescript": "~2.3.3",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/language-service": "4.3.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0"
  }
}
