{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./src",
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowUnreachableCode": true,
    "target": "ES2022",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "api-kit/*": [
        "api-kit/*"
      ],
      "app-components/*": [
        "app/app-components/*"
      ],
      "environment": [
        "./environments/environment.ts"
      ]
    },
    "lib": [
      "es2017",
      "dom"
    ],
    "useDefineForClassFields": false
  },
  "exclude": [
    "**/node_modules/**/*",
    "node_modules/**/*",
    "test.ts",
    "**/*.spec.ts"
  ]
}
